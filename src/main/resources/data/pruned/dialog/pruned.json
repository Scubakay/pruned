{
  "type": "minecraft:multi_action",
  "title": "Pruned",
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": {
        "text": "Manage if the current chunk is saved to the world download."
      }
    },
    {
      "type": "minecraft:plain_message",
      "contents": [
        "Saved in world download: ",
        {
          "score": {
            "objective": "pruned_current_region_is_saved",
            "name": "@s"
          },
          "type": "score",
          "bold": true
        }
      ]
    }
  ],
  "columns": 1,
  "actions": [
    {
      "label": "Save",
      "tooltip": "Adds the current chunk to the world download",
      "action": {
        "type": "minecraft:run_command",
        "command": "pruned save"
      }
    },
    {
      "label": "Remove",
      "tooltip": "Removes the current chunk from the world download",
      "action": {
        "type": "minecraft:run_command",
        "command": "pruned remove"
      }
    },
    {
      "label": "Toggle Region Manager",
      "tooltip": "Enables/disables chat messages showing if the current region is saved to the world download.",
      "action": {
        "type": "minecraft:run_command",
        "command": "pruned check"
      }
    },
    {
      "label": "WebDav Configuration",
      "tooltip": "Requires permission on servers",
      "action": {
        "type": "minecraft:run_command",
        "command": "pruned login webdav"
      }
    }
  ]
}