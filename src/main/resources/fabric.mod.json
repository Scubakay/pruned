{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "Scubakay"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${repository}",
    "issues": "${repository}/issues"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord}"
      }
    }
  },
  "license": "MIT",
  "icon": "assets/${id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.scubakay.pruned.PrunedMod"
    ],
    "client": [
      "com.scubakay.pruned.PrunedModClient"
    ]
  },
  "depends": {
    "fabricloader": ">=0.16",
    "minecraft": "${range}",
    "fabric-api": "*"
  }
}